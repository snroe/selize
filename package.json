{
  "name": "@selize/core",
  "version": "0.0.6",
  "author": "Snroe",
  "license": "MIT",
  "description": "selize core",
  "keywords": [
    "selize",
    "selize-core",
    "node",
    "bun",
    "server",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snroe/selize-core.git"
  },
  "bugs": "https://github.com/snroe/selize-core/issues",
  "type": "module",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "build": "rimraf lib && bun run check && bun run -c=bunfig.toml build.mjs && tsc --emitDeclarationOnly --outDir lib",
    "check": "tsc --noEmit",
    "test": "tsc --noEmit && cd __tests__ && bun test server.test.ts",
    "clean": "rimraf lib node_modules/.tmp",
    "publish": "npm publish --access public"
  },
  "engines": {
    "bun": "latest"
  },
  "dependencies": {
    "@selize/utils": "latest",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "klaw": "^4.1.0",
    "morgan": "^1.10.0",
    "rimraf": "^6.0.1",
    "through2": "^4.0.2",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/klaw": "^3.0.7",
    "@types/morgan": "^1.9.10",
    "@types/through2": "^2.0.41",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}